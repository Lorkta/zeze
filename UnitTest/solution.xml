<?xml version="1.0" encoding="utf-8"?>

<solution name="demo" ModuleIdAllowRanges="1-3,5">
	<bean name="Bean1">
		<enum name="Enum1" value="4"/>
		<variable id="1" name="V1" type="int" default="1"/> bean1comm
		bean1line2
		<variable id="2" name="V2" type="map" key="int" value="int"/> bean1v2
		bean1v2line2
	</bean>

	<module name="Module1" id="1">
		<bean name="Value">
			<variable id="1" default="1" name="s" type="int"/> com aa
		</bean>
		<beankey name="Key">
			<variable id="1" default="1" name="s" type="short"/> com 2
			com 2
		</beankey>

		<protocol name="Protocol1" id="1" argument="Value" handle="server,client"/>
		<rpc name="Rpc1" id="2" argument="Value" result="Value" handle="server"/>
		<protocol name="Protocol3" id="3" argument="demo.Module2.Value" handle="server,client"/>

		<!-- 配置移到其他地方 cache="1024" persistence="MEMORY" -->
		<table name="Table1" key="long" value="Value"/>

		<module name="Module11" id="3">
		</module>
	</module>

	<module name="Module2" id="2">
		<bean name="Value">
			<variable id="1" default="1" name="s" type="int"/> com aa
		</bean>
	</module>

	<project name="gsd" platform="cs" generates="table">
		<!--
		mudule 可以被多个 manager 引用，但只会生成一份.
		这样的话，协议处理代码需要根据当前manager做不同实现。
		生成的时候警告处理。
		-->
		<module ref="Module1"/>
		<manager name="Server" handle="server" class="demo.Provider">
			<module ref="Module2"/>
		</manager>
	</project>

	<project name="client" platform="cs" generates="">
		<!--
		mudule 可以被多个 manager 引用，但只会生成一份.
		这样的话，协议处理代码需要根据当前manager做不同实现。
		生成的时候警告处理。
		-->
		<module ref="Module1"/>
		<manager name="Client" handle="client" class="demo.ClientManager">
			<module ref="Module2"/>
		</manager>
	</project>
</solution>
